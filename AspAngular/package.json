{
    "name": "angular-quickstart",
    "version": "1.0.0",
    "description": "QuickStart package.json from the documentation, supplemented with testing support",
    "scripts": {
        "build": "tsc -p src/",
        "build:watch": "tsc -p src/ -w",
        "build:e2e": "tsc -p e2e/",
        "build:upgrade": "tsc",
        "serve:upgrade": "http-server",
        "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
        "serve:aot": "lite-server -c bs-config.aot.json",
        "build:babel": "babel src -d src --extensions \".es6\" --source-maps",
        "copy-dist-files": "node ./copy-dist-files.js",
        "i18n": "ng-xi18n",
        "serve": "lite-server -c=bs-config.json",
        "serve:e2e": "lite-server -c=bs-config.e2e.json",
        "prestart": "npm run build",
        "start1Lite": "concurrently \"npm run build:watch\" \"npm run serve\"",
        "startGulp": "concurrently \"npm run gulp\" \"npm run watch\" \"npm run tsc:w\"",
        "pree2e": "npm run build:e2e",
        "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
        "preprotractor": "webdriver-manager update",
        "protractor": "protractor protractor.config.js",
        "pretest": "npm run build",
        "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
        "pretest:once": "npm run build",
        "test:once": "karma start karma.conf.js --single-run",
        "lint": "tslint ./src/**/*.ts -t verbose",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "typings": "typings",
        "gulp": "gulp",
        "watch": "gulp watch",
        "ngc": "ngc",
        "getNpmVersion": "npm -v",
        "getNodeVersion": "node -v",
        "cleanNpmCache": "npm cache clean --force",
        "customInstall": "npm install",
        "updateNpm": "npm install npm@latest",
        "cmd": "npm typescript"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@angular/common": "~4.0.0",
        "@angular/compiler": "~4.0.0",
        "@angular/core": "~4.0.0",
        "@angular/forms": "~4.0.0",
        "@angular/http": "~4.0.0",
        "@angular/platform-browser": "~4.0.0",
        "@angular/platform-browser-dynamic": "~4.0.0",
        "@angular/router": "~4.0.0",
        "@angular/upgrade": "~4.0.0",
        "angular-in-memory-web-api": "~0.3.0",
        "systemjs": "0.19.40",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.8",
        "rxjs": "5.0.1",
        "zone.js": "^0.8.4",
        "bower": "1.7.9",
        "jquery": "^3.1.0"
    },
    "devDependencies": {
        "concurrently": "^3.2.0",
        "gulp": ">=3.9.1",
        "gulp-concat": ">=2.5.2",
        "gulp-copy": ">=0.0.2",
        "gulp-cssmin": ">=0.1.7",
        "gulp-load-plugins": "^1.3.0",
        "gulp-rename": ">=1.2.2",
        "gulp-rimraf": ">=0.2.0",
        "gulp-tsc": ">=1.2.0",
        "gulp-uglify": ">=1.2.0",
        "gulp-watch": ">=4.3.9",
        "gulp-clean-css": "^3.0.4",
        "gulp-clean": "^0.3.2",
        "lite-server": "^2.2.2",
        "typescript": "~2.1.0",
        "typings": "^1.3.2",
        "canonical-path": "0.0.2",
        "tslint": "^3.15.1",
        "lodash": "^4.16.4",
        "jasmine-core": "~2.4.1",
        "karma": "^1.3.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~4.0.14",
        "rimraf": "^2.5.4",
        "@types/core-js": "^0.9.34",
        "@types/node": "^6.0.45",
        "@types/jasmine": "2.5.36",
        "@types/angular": "^1.5.16",
        "@types/angular-animate": "^1.5.5",
        "@types/angular-cookies": "^1.4.2",
        "@types/angular-mocks": "^1.5.5",
        "@types/angular-resource": "^1.5.6",
        "@types/angular-route": "^1.3.2",
        "@types/angular-sanitize": "^1.3.3",
        "babel-cli": "^6.16.0",
        "babel-preset-angular2": "^0.0.2",
        "babel-preset-es2015": "^6.16.0",
        "http-server": "^0.9.0",
        "jasmine": "~2.4.1",
        "rollup": "^0.41.6",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-node-resolve": "2.0.0",
        "rollup-plugin-uglify": "^1.0.1",
        "source-map-explorer": "^1.3.2"
    },
    "repository": {}
}
